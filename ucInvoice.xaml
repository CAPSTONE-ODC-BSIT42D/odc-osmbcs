             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:prototype2"
             xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" xmlns:syncfusion="http://schemas.syncfusion.com/wpf" x:Name="UserControlInvoice" x:Class="prototype2.ucInvoice"
             mc:Ignorable="d" d:DesignHeight="824.333" d:DesignWidth="1068" IsVisibleChanged="UserControlInvoice_IsVisibleChanged" DataContext="{StaticResource MainVM}">
